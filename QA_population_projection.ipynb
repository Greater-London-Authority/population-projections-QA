    "    tally_df = tally_df.sort_values(by='count', ascending=False)\n",
    "\n",
    "    # Display the top rows of the tally DataFrame\n",
    "    print(tally_df)\n",
    "else:\n",
    "    print(\"No relevant data found.\")"
